{
  "preset": "react-native",
  "setupFiles": [
    "<rootDir>/__tests__/jest.setup.js"
  ],
  "testPathIgnorePatterns": [
    "/node_modules",
    "/ios",
    "/android",
    "<rootDir>/__tests__/jest.setup.js"
  ],
  "collectCoverage": false,
  "collectCoverageFrom": [
    "__tests__/**/*.{tsx,ts}",
    "src/**/*.{tsx,ts}",
    "!/src/**/*.spec.{tsx,ts}"
  ],
  "setupFilesAfterEnv": [ "@testing-library/jest-native/extend-expect"],
  "transformIgnorePatterns": [
    "node_modules/(?!@react-native|react-native)"
  ]
}
